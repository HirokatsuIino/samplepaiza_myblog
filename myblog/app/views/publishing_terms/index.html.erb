<p id="notice"><%= notice %></p>

<h1>Publishing Terms</h1>

<table>
  <thead>
    <tr>
      <th>Client publishing setting</th>
      <th>Begin at</th>
      <th>End at</th>
      <th>Flg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publishing_terms.each do |publishing_term| %>
      <tr>
        <td><%= publishing_term.client_publishing_setting_id %></td>
        <td><%= publishing_term.begin_at %></td>
        <td><%= publishing_term.end_at %></td>
        <td><%= publishing_term.flg %></td>
        <td><%= link_to 'Show', publishing_term %></td>
        <td><%= link_to 'Edit', edit_publishing_term_path(publishing_term) %></td>
        <td><%= link_to 'Destroy', publishing_term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publishing Term', new_publishing_term_path %>
