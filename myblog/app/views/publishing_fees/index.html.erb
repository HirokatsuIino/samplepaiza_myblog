<p id="notice"><%= notice %></p>

<h1>Publishing Fees</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @publishing_fees.each do |publishing_fee| %>
      <tr>
        <td><%= publishing_fee.client_id %></td>
        <td><%= publishing_fee.value %></td>
        <td><%= link_to 'Show', publishing_fee %></td>
        <td><%= link_to 'Edit', edit_publishing_fee_path(publishing_fee) %></td>
        <td><%= link_to 'Destroy', publishing_fee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Publishing Fee', new_publishing_fee_path %>
