<p id="notice"><%= notice %></p>

<h1>お知らせ管理</h1>

<br>
<%= link_to '新規登録', new_notification2_path %>
<br>


<table>
  <thead>
    <tr>
      <th>お知らせID</th>
      <th>題名</th>
      <th>表示</th>
      <th>最終更新日</th>
      <th colspan="3">操作</th>
    </tr>
  </thead>

  <tbody>
    <% @notification2s.each do |notification2| %>
      <tr>
        <td><%= notification2.id %></td>
        <td><%= notification2.title %></td>
        <% if notification2.begin_at > @now_date %>
          <td>ああああ</td>
        <% else %>
          <td>いいい</td>
        <% end %>
        <td><%= notification2.updated_at %></td>
        <td><%= link_to '詳細', notification2 %></td>
        <td><%= link_to '編集', edit_notification2_path(notification2) %></td>
<!--        <td><%#= link_to 'Destroy', notification2, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

