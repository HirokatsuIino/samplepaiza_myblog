<p id="notice"><%= notice %></p>

<h1>Notification2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Begin at</th>
      <th>End at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notification2s.each do |notification2| %>
      <tr>
        <td><%= notification2.title %></td>
        <td><%= notification2.body %></td>
        <td><%= notification2.begin_at %></td>
        <td><%= notification2.end_at %></td>
        <td><%= link_to 'Show', notification2 %></td>
        <td><%= link_to 'Edit', edit_notification2_path(notification2) %></td>
        <td><%= link_to 'Destroy', notification2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notification2', new_notification2_path %>
