<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Client</th>
      <th>Name</th>
      <th>Admin comment</th>
      <th>Area important</th>
      <th>Nav</th>
      <th>Nav</th>
      <th>Nav</th>
      <th>Nav</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.id %></td>
        <td><%= client.client_id %></td>
        <td><%= client.name %></td>
        <td><%= client.client_publishing_setting.comment %></td>
        <td><%= client.client_unit_price.unit_price_id %></td>
        <td><%= client.publishing_fee.value %></td>
<!--        <td><%#= client.client_unit_price.id %></td>-->
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
